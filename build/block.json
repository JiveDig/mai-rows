{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "mai/rows",
  "version": "0.1.0",
  "title": "Mai Rows",
  "category": "widgets",
  "icon": "smiley",
  "description": "Example block scaffolded with Create Block tool.",
  "attributes": {
    "columnsLg": {
      "type": "array",
      "default": {}
    },
    "columnsMd": {
      "type": "array",
      "default": {}
    }
  },
  "supports": {
    "html": false,
    "color": {
      "text": true,
      "background": true,
      "link": true
    },
    "spacing": {
      "margin": true,
      "padding": true,
      "blockGap": true
    }
  },
  "textdomain": "mai-rows",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js"
}